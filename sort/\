
#include "sort.h"


void swap(int *a, int *b)
{
	int temp = 0;

	temp = *a;
	*a = *b;
	*b = temp;
}

void swap(int *a, int *b)
{
	*a = *a + *b;
	*b = *a - *b;
	*a = *a - *b;
}

void sort(int *arr, int num)
{
	int i = 0, j = 0;

	for (i = 0; i < num; i++) {
		for (j = 1; j < num; j++) {

		}

	}

}

